<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/lname"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="left"
        android:layout_marginLeft="16dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="13dp"
        android:text="Name: "
        android:textColor="@color/common_google_signin_btn_text_dark_focused"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:id="@+id/name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="right"
        android:text="tname"
        android:textColor="@color/common_google_signin_btn_text_dark_focused"
        android:layout_below="@+id/lname"
        android:layout_alignLeft="@+id/lname"
        android:layout_alignStart="@+id/lname" />

    <TextView
        android:id="@+id/lphone"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="left"
        android:layout_marginTop="12dp"
        android:text="Phone: "
        android:textColor="@color/common_google_signin_btn_text_dark_focused"
        android:textSize="12sp"
        android:layout_below="@+id/name"
        android:layout_alignLeft="@+id/name"
        android:layout_alignStart="@+id/name" />

    <TextView
        android:id="@+id/phone"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/lphone"
        android:layout_alignStart="@+id/lphone"
        android:layout_below="@+id/lphone"
        android:layout_gravity="right"
        android:text="lphone"
        android:textColor="@color/common_google_signin_btn_text_dark_focused"
        android:textSize="12sp" />

    <TextView
        android:id="@+id/lemail"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="74dp"
        android:layout_marginStart="74dp"
        android:text="Email: "
        android:textColor="@color/common_google_signin_btn_text_dark_focused"
        android:textSize="12sp"
        android:layout_alignBaseline="@+id/lcity"
        android:layout_alignBottom="@+id/lcity"
        android:layout_toRightOf="@+id/lname"
        android:layout_toEndOf="@+id/lname" />

    <TextView
        android:id="@+id/email"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/lemail"
        android:layout_alignStart="@+id/lemail"
        android:layout_below="@+id/lemail"
        android:text="tmail"
        android:textColor="@color/common_google_signin_btn_text_dark_focused"
        android:textSize="12sp" />

    <TextView
        android:id="@+id/lstate"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/lphone"
        android:layout_alignBottom="@+id/lphone"
        android:layout_alignLeft="@+id/email"
        android:layout_alignStart="@+id/email"
        android:text="State: "
        android:textColor="@color/common_google_signin_btn_text_dark_focused"
        android:textSize="12sp" />

    <TextView
        android:id="@+id/state"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/lstate"
        android:layout_alignStart="@+id/lstate"
        android:layout_below="@+id/lstate"
        android:text="tstate"
        android:textColor="@color/common_google_signin_btn_text_dark_focused"
        android:textSize="12sp" />

    <TextView
        android:id="@+id/lcity"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="73dp"
        android:layout_marginStart="73dp"
        android:text="City: "
        android:textColor="@color/common_google_signin_btn_text_dark_focused"
        android:textSize="12sp"
        android:layout_alignBottom="@+id/lname"
        android:layout_toRightOf="@+id/lemail"
        android:layout_toEndOf="@+id/lemail" />

    <TextView
        android:id="@+id/city"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/lcity"
        android:layout_alignStart="@+id/lcity"
        android:layout_below="@+id/lcity"
        android:text="tcity"
        android:textColor="@color/common_google_signin_btn_text_dark_focused"
        android:textSize="12sp" />

    <TextView
        android:id="@+id/lStreet"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Street: "
        android:textColor="@color/common_google_signin_btn_text_dark_focused"
        android:textSize="12sp"
        android:layout_alignBaseline="@+id/lstate"
        android:layout_alignBottom="@+id/lstate"
        android:layout_alignLeft="@+id/city"
        android:layout_alignStart="@+id/city" />

    <TextView
        android:id="@+id/Street"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/lStreet"
        android:layout_alignStart="@+id/lStreet"
        android:layout_below="@+id/lStreet"
        android:text="tstreet"
        android:textColor="@color/common_google_signin_btn_text_dark_focused"
        android:textSize="12sp" />

    <TextView
        android:id="@+id/lcode"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Code: "
        android:textColor="@color/common_google_signin_btn_text_dark_focused"
        android:textSize="12sp"
        android:layout_marginLeft="45dp"
        android:layout_marginStart="45dp"
        android:layout_alignBaseline="@+id/lcity"
        android:layout_alignBottom="@+id/lcity"
        android:layout_toRightOf="@+id/lStreet"
        android:layout_toEndOf="@+id/lStreet" />

    <TextView
        android:id="@+id/code"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/lcode"
        android:layout_alignStart="@+id/lcode"
        android:layout_below="@+id/lcode"
        android:text="tcode"
        android:textColor="@color/common_google_signin_btn_text_dark_focused"
        android:textSize="12sp" />


</RelativeLayout>